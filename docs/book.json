{
  "author": "doctording",
  "description": "Spring源码学习笔记",
  "language": "zh-hans",
  "plugins": [
    "-highlight",
    "-lunr",
    "-search",
    "-sharing",
    "ace",
    "codeblock-filename",
    "search-plus",
    "sectionx",
    "splitter",
    "tbfed-pagefooter",
    "expandable-chapters",
    "back-to-top-button",
    "code",
    "uml",
    "page-treeview"
  ],
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright": "Copyright @doctording",
      "modify_label": "该文件修改时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "page-treeview": {
      "copyright": "Copyright @doctording",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    }
  },
  "styles": {
    "website": "styles/website.css"
  },
  "title": "spring-note"
}